.form-element__section
  .form-element__container
    .form-element__col
      .form-element__item-list
        .form-element__item
          +input-text({
            view:"",
            title:"text field",
            subtitle:"default",
            type:"email",
            placeholder:"Email",
          })

        .form-element__item
          +input-text({
            view:"hover",
            title:"text field",
            subtitle: "Hover / Focus",
            placeholder: "This is pretty awesome",
            name:"hover"
          })

        .form-element__item
          +guests-dropdown({
            title:"dropdown",
            subtitle:"",
            counts:[0,0,0],
            isOpened:false,
            withActions:true
          })

        .form-element__item
          +input-text({
            view:"masked",
            title:"masked text field",
            subtitle: "default",
            placeholder: "ДД.ММ.ГГГГ",
            name:"masked",
          })

      .form-element__item-list
        +input-text({
          view:"rangeDate",
          title:"",
          subtitle:"",
          arrival:"прибытие",
          departure:"выезд",
          placeholder:"ДД.ММ.ГГГГ"
        })

      .form-element__item-list
        +input-text({
          view:"filterDate",
          title:"filter date dropdown",
          subtitle: "",
        })

      .form-element__item-list
        +input-text({
          view:"arrow",
          title:"subscription text field",
          subtitle: "",
          placeholder: "Email",
          name:"email",
          type:"email"
        })

    .form-element__col
      .form-element__item-list
        +checkbox-list({
          title:"Checkbox buttons",
          subtitle: "",
          expand:false,
          items: [
              {
                richName: "",
                text: "Можно курить",
                dis:false,
              },
              {
                richName: "",
                text: "Можно с питомцами",
                check: true
              },
              {
                text: "Можно пригласить гостей (до 10 человек)",
                check: true,
                dis:true,
              }
            ]
          })

      .form-element__item-list Radio buttons
        .form-element__item-pack
          .form-element__item-box
            +radio({text:"Мужчина",name:"gender",value:"male",checked: true})
          .form-element__item-box
            +radio({text:"Женщина",name:"gender",value:"female"})

      .form-element__item-list Toogle
        .form-element__item-pack
          +toggle({text:"Получать спецпредложение", checked:true})

        .form-element__item-pack
          +toggle({text:"Получать спецпредложение"})

      .form-element__item-list Like buttons
        .form-element__item-pack
          .form-element__item-box
            +like({name:"количество лайков", count:10})
          .form-element__item-box
            +like({name:"количество лайков", like:true, count:12})

      .form-element__item-list Rate button
        .form-element__item-pack
          .form-element__item-box
            +rating({title:"", rate:4, name: "рейтинг 1 номера"})
          .form-element__item-box
            +rating({title:"", rate:5, name: "рейтинг 2 номера"})

    .form-element__col
      .form-element__item-list
        .form-element__item
          +range-slider({title:"Range slider", subtitle:"5 000р-10000р", min:0, max:15000, from:5000,to:10000})

      .form-element__item-list Buttons
        .form-element__item-pack
          +button({theme:"fill",text:"click me"})

      .form-element__item
          +button({text:"click me"})

      .form-element__item
          +button({theme: "string", text:"click me"})

      .form-element__item
          +button({theme: "arrow", size:" ", text:"Перейти к оплате"})

      .form-element__item-list Pagination
        .form-element__item-pack
          +pagination({current:1, total:15, variant:100})

  .form-element__container
    .form-element__col
      .form-element__item-list
        .form-element__item
          +furniture-dropdown({
            subtitle:"Default"
          })

        .form-element__item
          +furniture-dropdown({
            subtitle:"Expanded",
            isOpened:true,
            withActions:false,
          })

    .form-element__col
      .form-element__item-list
        .form-element__item
          +guests-dropdown({
            title:"dropdown",
            isOpened:true,
          })

    .form-element__col
      .form-element__item-list
        .form-element__item
          +guests-dropdown({
            title:"dropdown",
            counts:[2,1,0],
            isOpened:true,
          })

  .form-element__container
    .form-element__col
      +checkbox-list({
        title:"Дополнительные удобства",
        subtitle: "",
        expand:true,
        items:[
            {text: "Завтрак"},
            {text: "Письменный стол", check: true},
            {text: "Стул для кормления", check: true},
            {text: "Кроватка", check: true},
            {text: "Телевизор"},
            {text: "Шампунь"},
            {text: "Интернет"},
            {text: "Связь с космосом"}
        ]
      })

    .form-element__col
      +checkbox-list({
        title:"Rich Checkbox Buttons",
        items:[
            {
              richName: "Широкий коридор",
              text: "Ширина коридоров в номере не менее 91 см."
            },
            {
              richName: "Помощник для инвалидов",
              text: "На 1 этаже вас встретит специалист  и проводит до номера."
            }
          ]
        })

    .form-element__col
      .form-element__list-item Bullet list
        +info({
          title:"",
          subtitle:" ",
          items:[
            {
              bullet:true,
              text:"Нельзя с питомцами"
            },
            {
              bullet:true,
              text:"Без вечеринок и мероприятий"
            },
            {
              bullet:true,
              text:"Время прибытия — после 13:00, а выезд до 12:00"
            },
          ]
        })

  .form-element__container
    .form-element__col
      +details({
            title:"",
            subtitle: "",
            items: [
                {
                  icon:"smile",
                  name: "Комфорт",
                  text:"Шумопоглощающие стены",
                },
                {
                  icon: "city",
                  name: "Удобство",
                  text: "Окно в каждой из спален"
                },
              ]
            })

    .form-element__col_expanded
      +reviews({
          title:"",
          subtitle: "",
          review: [
              {
                avatar:require("../ui-kit/form-elements/images/author_1.png").default,
                like:true,
                count: 12,
                author:"Мурад Сарафанов",
                time:"5 дней назад ",
                text:"Великолепный матрас на кровати в основной спальне! А пуфик вообще потрясающий. И стены, действительно, шумоподавляющие. Выкрикивал комплименты повару — никто не жаловался из соседей.  "
              },
            ]
          })
